let rec firstNat x=
		match x with
		|0 -> []
		|x -> x-1::(firstNat (x-1))
	
;;

firstNat 7

let rec pack x=
		match x with
		| [] -> []
		| (x,y)::(w,z)::trail -> if len x=0 then 
																	(head,1)::(pack secondHead::trail)  
																	else
																		if head=secondHead then
																			
																		
;;

let rec len x=
		match x with
		| [] ->0
		| head::trail-> 1+ len trail
;;